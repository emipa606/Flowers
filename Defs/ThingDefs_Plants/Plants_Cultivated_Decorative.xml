<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<!--=========================== Daisy ==============================-->
	
  <ThingDef ParentName="PlantBase">
    <defName>Plant_Daisy</defName>
    <label>daisy</label>
    <description>A beautiful cultivated flower.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Beauty>12</Beauty>
      <Nutrition>0.05</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Daisy</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
    <ingestible />
     <plant>
		<dieIfLeafless>true</dieIfLeafless>
		<harvestTag>Standard</harvestTag>
		<sowWork>800</sowWork>
		<sowTags>
			<li>Ground</li>
			<li>Decorative</li>
		</sowTags>
		<growMinGlow>0.3</growMinGlow>
		<topWindExposure>0.1</topWindExposure>
		<growDays>1.5</growDays>
		<lifespanDaysPerGrowDays>5</lifespanDaysPerGrowDays>
		<visualSizeRange>
        <min>0.3</min>
        <max>1.05</max>
      </visualSizeRange>
      <wildOrder>1</wildOrder>
      <purpose>Beauty</purpose>
    </plant>
  </ThingDef>
  
